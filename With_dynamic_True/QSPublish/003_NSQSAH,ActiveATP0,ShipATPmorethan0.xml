<?xml version="1.0" encoding="UTF-8"?>		 
<!-- Consider an item with NS QS = AH, Active ATP = 0 , ShipATP > 0. Validate entry made into jcp_capacity_exclusions.
And call PublishCapacityBasedAPI to force insert record in jcp_capacity_exclusions.

-->

<testsuite description="positive flow : Consider an item with NS QS = AH, Active ATP = 0 , ShipATP > 0. Validate entry made into jcp_capacity_exclusions" name="003">

	 <test apiMethod="POST" apiUrl="/master/node/JCP/2997/" 
		description="Insert node 1"
        expectedJsonFile="expected\003\003_01.json"
        inputJsonFile="input\003\003_01.json" name="01" outputJsonFile="output\003\003_01.json"/>

	<test apiMethod="POST" apiUrl="/master/itemDetails/JCP/" 
		description="item details"
        expectedJsonFile="expected\003\003_02.json"
        inputJsonFile="input\003\003_02.json" name="02" outputJsonFile="output\003\003_02.json"/>

	<test apiMethod="POST" apiUrl="/inventory/JCP/absolute/" 
		description="insert inventory"
        expectedJsonFile="expected\003\003_03.json"
        inputJsonFile="input\003\003_03.json" name="03" outputJsonFile="output\003\003_03.json"/>

    <test apiMethod="POST" apiUrl="/master/masterdata/insert/nodeCapacityDetails/JCP/" 
		description="insert capacity"
        expectedJsonFile="expected\003\003_04.json"
        inputJsonFile="input\003\003_04.json" name="04" outputJsonFile="output\003\003_04.json"/>
		
	<test apiMethod="POST" apiUrl="/inventory/multiread/JCP/" 
		description="multiread"
		expectedJsonFile="expected\003\003_05.json"
		inputJsonFile="input\003\003_05.json" name="05" outputJsonFile="output\003\003_05.json"/>
		
	<test apiMethod="POST" apiUrl="/master/qualitativeStatus/" 
		description="insert qualitativeStatus"
		expectedJsonFile="expected\003\003_06.json"
		inputJsonFile="input\003\003_06.json" name="06" outputJsonFile="output\003\003_06.json"/>
	

					<test apiMethod="POST" apiUrl="/master/mapValue/jcp_location_inventory_pojo/" 
						description="location inventory"
						expectedJsonFile="expected\003\003_07.json"
						inputJsonFile="input\003\003_07.json" name="07" outputJsonFile="output\003\003_07.json"/>
						
					<test apiMethod="delay" delay="15000" description="Delay : 15000 ms" name="99"/> 
						
		<test apiMethod="POST" apiUrl="/master/getqualitativeStatus/" 
		description="get qualitativeStatus"
		expectedJsonFile="expected\003\003_08.json"
		inputJsonFile="input\003\003_08.json" name="08" outputJsonFile="output\003\003_08.json"/>		

					<test apiMethod="delay" delay="15000" description="Delay : 15000 ms" name="100"/>  
						
					<test apiMethod="POST" apiUrl="/master/mapValue/jcp_capacity_exclusions" 
						description="Check entry in jcp_capacity_exclusions map"
						expectedJsonFile="expected\003\003_09.json"
						inputJsonFile="input\003\003_09.json" name="09" outputJsonFile="output\003\003_09.json"/>
					
		<test apiMethod="POST" apiUrl="/master/publishCapacityBasedQS/77012131003" 
		description="Force publishCapacityBasedQS"
		expectedJsonFile="expected\003\003_10.json"
		inputJsonFile="" name="10" outputJsonFile="output\003\003_10.json"/>
						
						
					<test apiMethod="POST" apiUrl="/master/mapValue/jcp_capacity_exclusions" 
						description="Check entry in jcp_capacity_exclusions map"
						expectedJsonFile="expected\003\003_11.json"
						inputJsonFile="input\003\003_11.json" name="11" outputJsonFile="output\003\003_11.json"/>
</testsuite>
